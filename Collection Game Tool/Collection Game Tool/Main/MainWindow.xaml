<Window x:Class="Collection_Game_Tool.Main.Window1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:gs="clr-namespace:Collection_Game_Tool.GameSetup"
        xmlns:pl="clr-namespace:Collection_Game_Tool.PrizeLevels"
        xmlns:dv="clr-namespace:Collection_Game_Tool.Divisions"
        Title="Collection Game Tool" WindowState="Maximized" 
        SizeToContent="WidthAndHeight" WindowStartupLocation="CenterScreen">
    <StackPanel>
        <StackPanel Orientation="Horizontal" Background="white">
            <Image Source="logo.png" Margin="10"></Image>
            <Label Foreground="#007CAA" Background="white" FontSize="48" FontWeight="Bold">Collection Game Generator</Label>

        </StackPanel>
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Stretch">
            <pl:UserControlPrizeLevels></pl:UserControlPrizeLevels>
            <gs:GameSetupUC></gs:GameSetupUC>
            <gs:GameSetupUC></gs:GameSetupUC>

        </StackPanel>
        
        
    </StackPanel>
</Window>
